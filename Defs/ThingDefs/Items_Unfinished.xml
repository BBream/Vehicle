<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <ThingDef Name="UnfinishedBase" Abstract="True">
    <thingClass>UnfinishedThing</thingClass>
    <category>Item</category>
    <label>Unfinished thing</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <useHitPoints>true</useHitPoints>
    <isUnfinishedThing>true</isUnfinishedThing>
    <selectable>true</selectable>
    <tradeability>Never</tradeability>
    <drawerType>RealtimeOnly</drawerType>
    <passability>Impassable</passability>
    <!-- For CompForbiddable -->
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
    </statBases>
    <comps>
      <li>
        <compClass>CompForbiddable</compClass>
      </li>
    </comps>
    <alwaysHaulable>false</alwaysHaulable>
    <rotatable>true</rotatable>
    <pathCost>15</pathCost>
    <thingCategories>
      <li>Unfinished</li>
    </thingCategories>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>

  <ThingDef ParentName="UnfinishedBase">
    <defName>UnfinishedAbstract</defName>
    <label>Unfinished abstract</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/UnfinishedWeapon</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1)</drawSize>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <comps>
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
  </ThingDef>
  
  <ThingDef ParentName="UnfinishedBase">
    <defName>UnfinishedVehicleAPC</defName>
    <label>Unfinished vehicle APC</label>
    <graphicData>
      <texPath>Things/Pawn/Vehicle/APC</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,3)</drawSize>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <size>(2,3)</size>
    <comps>
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
  </ThingDef>
  
  <ThingDef ParentName="UnfinishedBase">
    <defName>UnfinishedVehicleSmallTruck</defName>
    <label>Unfinished vehicle small truck</label>
    <graphicData>
      <texPath>Things/Pawn/Vehicle/SmallTruck</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <size>(1,2)</size>
    <comps>
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
  </ThingDef>

</Defs>